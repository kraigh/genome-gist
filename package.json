{
  "name": "genomegist",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ignore-pattern src/cli",
    "lint:cli": "tsc --project tsconfig.cli.json --noEmit",
